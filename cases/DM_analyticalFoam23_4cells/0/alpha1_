    FoamFile
    {
        version 2.0;
        format ascii;
        class volScalarField;
        object alpha1_;
    }
    // * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
    dimensions [0 0 0 0 0 0 0];
    internalField uniform 0;  //1;  //
    boundaryField
    { 
        wall
        {
            type zeroGradient;
        }

        outlet
        {
            type advective;
            //type inletOutlet;
            //inletValue 0;
        }
    
        inlet
        {
            type fixedValue;
            value uniform 1;
        }
    }
