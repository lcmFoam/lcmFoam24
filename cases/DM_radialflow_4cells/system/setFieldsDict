FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.resin 0
);

regions
(
    cylinderToCell
    {
        action      new;
        p1	   (0 0 -1);
        p2         (0 0 1);
        radius      0.007;


        fieldValues
        (
            volScalarFieldValue alpha.resin 1
        );
    }
);


