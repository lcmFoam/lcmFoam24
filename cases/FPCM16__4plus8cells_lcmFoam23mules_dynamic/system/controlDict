FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     lcmFoam23;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         40;  //160;

deltaT          1e-9;  //0.001;

writeControl    adjustableRunTime;

writeInterval   1;  //10;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  true;

maxCo           0.5;
maxAlphaCo      0.5;

maxDeltaT       1;


